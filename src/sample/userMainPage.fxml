<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="337.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.userMainPageController">
    <children>
        <ImageView fitHeight="367.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../background_images/5c66e24577597b326f81272689c4989a.gif"/>
            </image>
        </ImageView>
        <Button layoutX="45.0" layoutY="52.0" mnemonicParsing="false" onAction="#viewAllCars" prefHeight="41.0"
                prefWidth="267.0" text="View All cars" textFill="#0061ff">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
        </Button>
        <Button layoutX="45.0" layoutY="114.0" mnemonicParsing="false" onAction="#idSearch" prefHeight="41.0"
                prefWidth="267.0" text="Search car by Registration Number" textAlignment="CENTER" textFill="#0061ff"
                wrapText="true">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
        </Button>
        <Button layoutX="45.0" layoutY="177.0" mnemonicParsing="false" onAction="#modelSearch" prefHeight="41.0"
                prefWidth="267.0" text="Search car by model and make" textAlignment="CENTER" textFill="#0061ff"
                wrapText="true">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
        </Button>
        <Button layoutX="45.0" layoutY="242.0" mnemonicParsing="false" onAction="#buyCar" prefHeight="41.0"
                prefWidth="267.0" text="Buy a car" textFill="#0061ff">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
        </Button>
        <JFXButton id="font-button" layoutX="504.0" layoutY="283.0" onAction="#exit" prefHeight="31.0" prefWidth="64.0"
                   stylesheets="@../css/button.css" text="Exit"/>
    </children>
</AnchorPane>
