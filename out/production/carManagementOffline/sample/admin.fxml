<?xml version="1.0" encoding="UTF-8"?>


<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="338.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.admin">
    <children>
        <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../carimages/maxresdefault.jpg"/>
            </image>
        </ImageView>
        <JFXButton id="font-button" fx:id="viewAll" layoutX="203.0" layoutY="115.0" onAction="#viewAll"
                   stylesheets="@../css/button.css" text="View All Manufacturers"/>
        <JFXButton id="font-button" fx:id="add" layoutX="198.0" layoutY="182.0" onAction="#add"
                   stylesheets="@../css/button.css" text="Add a new Manufacturer"/>
        <Label layoutX="135.0" layoutY="40.0" text="Admin Section" textFill="#fffdfd">
            <font>
                <Font name="Bookman Old Style" size="42.0"/>
            </font>
        </Label>
        <Button id="font-button" layoutX="485.0" layoutY="257.0" mnemonicParsing="false" onAction="#exit_"
                stylesheets="@../css/button.css" text="Quit"/>
    </children>
</AnchorPane>
