        <Button layoutX="253.0" layoutY="219.0" mnemonicParsing="false" onAction="#enter" text="Enter"/>
        <Label layoutX="126.0" layoutY="110.0" prefHeight="21.0" prefWidth="59.0" text="Model">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
        </Label>
        <Label layoutX="126.0" layoutY="167.0" text="CarMake">
